<!--<h1>Hi, <%#= current_user.first_name %></h1>-->
 <!--Hero section-->
<section class="hero index">
   <p class="notice"><%= notice %></p>
  <p><%= alert %></p>
   <!--Header-->
         <%= render "shared/nav", location: 'top' %>
       <%= login_helper %>
   <!--End header-->
   <!--Inner hero-->
   <div class="inner-hero">
      <canvas class="background-img gradient-hero">
         <%= image_tag("patt.png") %>
      </canvas>
      <!--Container-->
      <div class="container hero-content index">
         <!--Row-->
         <div class="row">
            <div class="col-sm-12 ">
               <!--Profile image-->
               <div class="profile-img ">
                  <%= image_tag("patt.png") %>
               </div>
               <!--End profile image-->
               <!--Profile name-->
               <div class="profile-name mt-30">
                  <h2>Elizabeth<span>Web designer - Developer</span></h2>
               </div>
               <!--End profile name-->
               <!--Profile social-->
               <div class="profile-social mt-30">
                  <ul class="social ">
                     <li><a href="#"><i class="icon-facebook"></i></a></li>
                     <li><a href="#"><i class="icon-vimeo"></i></a></li>
                     <li><a href="#"><i class="icon-twitter"></i></a></li>
                     <li><a href="#"><i class="icon-instagram"></i></a></li>
                  </ul>
               </div>
               <!--End profile social-->
               <a class="top scroll" href="#about"><i class="icon-down-open"></i></a>
            </div>
         </div>
         <!--End row-->
    </div>
    <!--End container-->
  </div>
  <!--End inner hero-->
</section>
<!--End hero section-->
<!-- BEGIN PORTFOLIO BOXED MASONRY 3 COLUMNS -->
<section class="section-portfolio section-overlapping">
  <div class="container-large">
    <ul class="items-filter filter-light p-l-10">
      <li>
        <a href="#" data-filter=".photography">Photography</a>
      </li>
      <li>
        <a href="#" data-filter=".design">Design</a>
      </li>
      <li>
        <a href="#" data-filter=".branding">Branding</a>
      </li>
      <li class="filter-all p-r-10">
        <a href="#" data-filter="*" class="current">Show all</a>
      </li>
    </ul>
    <div class="masonry grid no-space grid-3">
    <% @portfolio_items.each do |item| %>
   
      <div class="item design">
        <div class="item-wrapper">
          <figure class="he-1 blue darken-1" data-link="portfolio-single-extended">
            <%= image_tag item.thumb_image if !item.thumb_image.nil? %>
            <figcaption class="caption-dark">
              <h4><%= item.title %></h4>
              <p><%= item.subtitle %></p>
            </figcaption>
          </figure>
        </div>
      </div>
    <%end %>
    </div>
  </div>
</section>
<!-- END PORTFOLIO BOXED MASONRY 3 COLUMNS -->